/**index.wxss**/

.swiperList {
    width: 750rpx;
    height: 340rpx;
}

.swiperList image {
    width: 100%;
}

.cate-list {
    display: flex;
}

.cate-list navigator {
    padding: 20rpx;
    flex: 1;
}

.cate-list navigator image {
    width: 100%;
}

.floor .floor-list .floor-title {
    padding: 10rpx 0;
}

.floor .floor-list .floor-item {
    overflow: hidden;
}

.floor .floor-list .floor-item navigator {
    float: left;
    width: 33.33%;
}

.floor .floor-list .floor-item navigator:nth-last-child(-n+4) {
    height: 27.72711207vw;
    border-left: 10rpx solid #fff;
  }

.floor .floor-list .floor-item navigator:nth-last-child(3),
.floor .floor-list .floor-item navigator:nth-last-child(4) {
    border-bottom: 10rpx solid #fff;
}

.floor .floor-list .floor-item navigator image {
    width: 100%;
    height: 100%;
}